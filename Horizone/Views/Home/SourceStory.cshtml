@model IEnumerable<Horizone.Models.SourceStory>

@{
    ViewBag.Title = @StaticResource.Resources.SourceStory;
}

<h2>@StaticResource.Resources.SourceStory</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Source)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Source)
        </td>       
    </tr>
}

</table>
