@model IEnumerable<Horizone.Models.NamePlace>

@{
    ViewBag.Title = @StaticResource.Resources.NamePlaces;
}

<h2>@StaticResource.Resources.NamePlaces</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PlaceEn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlaceFr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlaceZh)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PlaceEn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PlaceFr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PlaceZh)
        </td>
       
    </tr>
}

</table>
