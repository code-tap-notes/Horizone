@model IEnumerable<Horizone.Models.LaidLinesRegularity>

@{
    ViewBag.Title = @StaticResource.Resources.LaidLinesRegularity;
}

<h2>@StaticResource.Resources.LaidLinesRegularity</h2>

<p>
    @Html.ActionLink(@StaticResource.Resources.Add, "Create", null, new { @class = "btn btn-primary" })
</p>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LaidLinesRegularityEn) @StaticResource.Resources.English
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LaidLinesRegularityFr) @StaticResource.Resources.French
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LaidLinesRegularityZh) @StaticResource.Resources.Chinese
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LaidLinesRegularityEn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LaidLinesRegularityFr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LaidLinesRegularityZh)
            </td>
            <td>
                @Html.ActionLink(@StaticResource.Resources.Edit, "Edit", new { id = item.Id }, new { @class = "btn btn-info", style = "width:85px" })
                @Html.ActionLink(@StaticResource.Resources.Details, "Details", new { id = item.Id }, new { @class = "btn btn-info", style = "width:85px" })
                @Html.ActionLink(@StaticResource.Resources.Delete, "Delete", new { id = item.Id }, new { @class = "btn btn-info", style = "width:85px" })
            </td>

        </tr>
    }

</table>
