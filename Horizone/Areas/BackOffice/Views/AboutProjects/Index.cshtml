@model IEnumerable<Horizone.Models.AboutProject>

@{
    ViewBag.Title = @StaticResource.Resources.AboutProject;
}

<h2>@StaticResource.Resources.AboutProject</h2>

<p>
    @Html.ActionLink(@StaticResource.Resources.Add, "Create", null, new { @class = "btn btn-primary" })
</p>
@foreach (var item in Model)
{
    <dl class="dl-horizontal">
        <dt>
            @StaticResource.Resources.Language
        </dt>
        <dd>
            @Html.DisplayFor(modelItem => item.Language.Symbol)
        </dd>
        <dt>
            @StaticResource.Resources.Aim
        </dt>
        <dd>
            @Html.Raw(item.Aim)
        </dd>
        <dt>
            @StaticResource.Resources.Funding
        </dt>
        <dd>
            @Html.Raw(item.Funding)
        </dd>
        <dt>
            @StaticResource.Resources.Programing
        </dt>
        <dd>
            @Html.Raw(item.Programing)
        </dd>
        <dt>
            @StaticResource.Resources.Feedback
        </dt>
        <dd>
            @Html.Raw(item.Feedback)
        </dd>
        <dt>
            @StaticResource.Resources.Contact
        </dt>
        <dd>
            @Html.Raw(item.Contact)
        </dd>
    </dl>

    <dl>
                @Html.ActionLink(@StaticResource.Resources.Edit, "Edit", new { id = item.Id }, new { @class = "btn btn-info" })
                @Html.ActionLink(@StaticResource.Resources.Details, "Details", new { id = item.Id }, new { @class = "btn btn-info" })
                @Html.ActionLink(@StaticResource.Resources.Delete, "Delete", new { id = item.Id }, new { @class = "btn btn-info" })

     </dl>
}       
 

   